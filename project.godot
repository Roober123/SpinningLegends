config_version=5

[application]

config/name="BeyRev"
run/main_scene="res://Menu/menu_screen.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/image="res://Resources/flashscreeen-0004.png"
boot_splash/use_filter=false
config/icon="res://Resources/photo.png"
config/windows_native_icon="res://Resources/photo.ico"

[autoload]

